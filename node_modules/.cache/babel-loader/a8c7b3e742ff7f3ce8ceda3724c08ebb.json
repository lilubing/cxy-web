AlB,IAAI,CAACmB,QAAL,GAAgB,UAASC,GAAT,EAAcC,IAAd,EAAoB;AAClC,MAAIC,CAAC,GAAG;AACN,UAAMD,IAAI,CAACE,QAAL,KAAkB,CADlB;AACqB;AAC3B,UAAMF,IAAI,CAACG,OAAL,EAFA;AAEgB;AACtB,UAAMH,IAAI,CAACI,QAAL,EAHA;AAGiB;AACvB,UAAMJ,IAAI,CAACK,UAAL,EAJA;AAImB;AACzB,UAAML,IAAI,CAACM,UAAL,EALA;AAKmB;AACzB,UAAMC,IAAI,CAACC,KAAL,CAAW,CAACR,IAAI,CAACE,QAAL,KAAkB,CAAnB,IAAwB,CAAnC,CANA;AAMuC;AAC7C,SAAKF,IAAI,CAACS,eAAL,EAPC,CAOsB;;AAPtB,GAAR;;AASA,MAAI,OAAOC,IAAP,CAAYX,GAAZ,CAAJ,EAAsB;AACpBA,IAAAA,GAAG,GAAGA,GAAG,CAACY,OAAJ,CAAYC,MAAM,CAACC,EAAnB,EAAuB,CAACb,IAAI,CAACc,WAAL,KAAqB,EAAtB,EAA0BC,MAA1B,CAAiC,IAAIH,MAAM,CAACC,EAAP,CAAUG,MAA/C,CAAvB,CAAN;AACD;;AACD,OAAK,IAAIC,CAAT,IAAchB,CAAd,EAAiB;AACf,QAAI,IAAIW,MAAJ,CAAW,MAAMK,CAAN,GAAU,GAArB,EAA0BP,IAA1B,CAA+BX,GAA/B,CAAJ,EAAyC;AACvCA,MAAAA,GAAG,GAAGA,GAAG,CAACY,OAAJ,CAAYC,MAAM,CAACC,EAAnB,EAAwBD,MAAM,CAACC,EAAP,CAAUG,MAAV,KAAqB,CAAtB,GACxBf,CAAC,CAACgB,CAAD,CADuB,GACf,CAAC,OAAOhB,CAAC,CAACgB,CAAD,CAAT,EAAcF,MAAd,CAAqB,CAAC,KAAKd,CAAC,CAACgB,CAAD,CAAP,EAAYD,MAAjC,CADR,CAAN;AAED;AACF;;AACD,SAAOjB,GAAP;AACD,CApBD;;AAqBA,eAAepB,IAAf","sourcesContent":["import log from './util.log.js'\nimport cookies from './util.cookies.js'\n\nconst util = {\n  cookies,\n  log\n}\n\n/**\n * @description 更新标题\n * @param {String} title 标题\n */\nutil.title = function(titleText) {\n  const processTitle = process.env.VUE_APP_TITLE || 'D2Admin'\n  window.document.title = `${processTitle}${titleText ? ` | ${titleText}` : ''}`\n}\n\n/**\n * @description 打开新页面\n * @param {String} url 地址\n */\nutil.open = function(url) {\n  var a = document.createElement('a')\n  a.setAttribute('href', url)\n  a.setAttribute('target', '_blank')\n  a.setAttribute('id', 'd2admin-menu-link')\n  document.body.appendChild(a)\n  a.click()\n  document.body.removeChild(document.getElementById('d2admin-menu-link'))\n}\n\nutil.Dtformat = function(fmt, date) {\n  var o = {\n    'M+': date.getMonth() + 1, // 月份\n    'd+': date.getDate(), // 日\n    'h+': date.getHours(), // 小时\n    'm+': date.getMinutes(), // 分\n    's+': date.getSeconds(), // 秒\n    'q+': Math.floor((date.getMonth() + 3) / 3), // 季度\n    'S': date.getMilliseconds() // 毫秒\n  }\n  if (/(y+)/.test(fmt)) {\n    fmt = fmt.replace(RegExp.$1, (date.getFullYear() + '').substr(4 - RegExp.$1.length))\n  }\n  for (var k in o) {\n    if (new RegExp('(' + k + ')').test(fmt)) {\n      fmt = fmt.replace(RegExp.$1, (RegExp.$1.length === 1)\n        ? (o[k]) : (('00' + o[k]).substr(('' + o[k]).length)))\n    }\n  }\n  return fmt\n}\nexport default util\n"]}]}