ndRoleAss',\n    method: 'post',\n    params\n  })\n}\n\nexport function getMenuButtonsTreeByRoleId(roleId) {\n  return request({\n    url: `/mybatis-web/menuButtons/menuButtonsTreeByRoleId/${roleId}`,\n    method: 'get'\n  })\n}\n\nexport function queryMenuList(roleId) {\n  return request({\n    url: '/mybatis-web/roles/menu/list',\n    method: 'get',\n    params: { roleId }\n  })\n}\n"]}]}